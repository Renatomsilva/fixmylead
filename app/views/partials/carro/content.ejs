<div class="row">
  <div class="col-md-2 col-md-offset-5">
    <h3 class="page-header">Pesquisa</h1>
  </div>
  <div class="col-md-2 col-md-offset-5">
    <form class="form-inline" ng-submit="vm.save(vm.carro)" name="searchForm">
      <label for="email">Placa</label>
      <input type="searchPlace" class="form-control" id="searchPlace" ng-model="vm.search.placa">
      </br>
      <button type="submit" class="form-control">Pesquisar</button>
    </form>
  </div>
</div>
<div class="row">
  <div class="col-md-2 col-md-offset-5">
    <h2 class="page-header">Cadastro do Carro</h1>
  </div>
  <div class="col-md-2 col-md-offset-5">
    <form class="form-inline" ng-submit="vm.save(vm.carro)" name="saveForm">
      <div class="form-group">
        <label for="marca">Marca</label>
        <input type="text" class="form-control" name="marca" ng-model="vm.carro.marca">
      </div>
      <div class="form-group">
        <label for="marca">Modelo</label>
        <input type="text" class="form-control" name="modelo" ng-model="vm.carro.modelo">
      </div>
      <div class="form-group">
        <label for="marca">Vers√£o</label>
        <input type="text" class="form-control" name="versao" ng-model="vm.carro.versao">
      </div>
      <div class="form-group">
        <label for="marca">Ano</label>
        <input type="text" class="form-control" name="ano" ng-model="vm.carro.ano">
      </div>
      <div class="form-group">
        <label for="marca">Placa</label>
        <input type="text" class="form-control" name="placa" ng-model="vm.carro.placa">
      </div>
      <div class="form-group">
        <button type="submit" class="form-control">Submit</button>
      </div>
    </form>
  </div>
</div>